#!/usr/bin/env bash

source utils

./git/setup
./intellij/setup
./tmux/setup
./vim/setup
./zsh/setup

if is_mac_os; then
  printf 'Installing brew packages/casks...\n'
  ./terminal/setup
  ./brew
elif is_arch; then
  printf 'Installing pacman packages...\n'
  ./pacman
fi

./vscode/setup

printf 'Installation has finished'

exec zsh
